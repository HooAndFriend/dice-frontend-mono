{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clear:port": "kill-port 3000 3001 3002",
    "dev": "pnpm clear:port && turbo run dev --parallel --no-cache",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker:build": "docker build --no-cache --rm -f Dockerfile -t pinomaker/dice-frontend:latest-arm64 .",
    "docker:push": "docker push pinomaker/dice-frontend:latest-arm64"
  },
  "devDependencies": {
    "@editorjs/header": "^2.8.7",
    "@editorjs/paragraph": "^2.11.6",
    "kill-port": "^2.0.1",
    "prettier": "^3.1.0",
    "turbo": "^2.0.3"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
